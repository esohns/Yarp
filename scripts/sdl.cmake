if (SDL1_FOUND OR SDL2_FOUND OR SDL3_FOUND)
 option (SDL_SUPPORT "SDL support" TRUE)
 if (SDL_SUPPORT)
  add_definitions (-DSDL_SUPPORT)
 endif (SDL_SUPPORT)
endif (SDL1_FOUND OR SDL2_FOUND OR SDL3_FOUND)

if (SDL_SUPPORT)
 # *NOTE*: the first entry is the default option
 if (SDL3_FOUND)
   set (SDL_VERSION "SDL3" CACHE STRING "use SDL3 (default)")
   set (SUPPORTED_SDL_VERSIONS "SDL3")
 elseif (SDL2_FOUND)
  set (SDL_VERSION "SDL2" CACHE STRING "use SDL2 (default)")
  set (SUPPORTED_SDL_VERSIONS "SDL2")
 elseif (SDL1_FOUND)
  set (SDL_VERSION "SDL" CACHE STRING "use SDL (default)")
  set (SUPPORTED_SDL_VERSIONS "SDL")
 endif ()
 if (SDL1_FOUND)
  set (SUPPORTED_SDL_VERSIONS "${SUPPORTED_SDL_VERSIONS};SDL")
 endif (SDL1_FOUND)
 if (SDL2_FOUND)
  set (SUPPORTED_SDL_VERSIONS "${SUPPORTED_SDL_VERSIONS};SDL2")
 endif (SDL2_FOUND)
 if (SDL3_FOUND)
  set (SUPPORTED_SDL_VERSIONS "${SUPPORTED_SDL_VERSIONS};SDL3")
 endif (SDL3_FOUND)
 list (REMOVE_DUPLICATES SUPPORTED_SDL_VERSIONS)
 set_property (CACHE SDL_VERSION PROPERTY STRINGS ${SUPPORTED_SDL_VERSIONS})

 if (NOT DEFINED SDL_VERSION_LAST)
  set (SDL_VERSION_LAST "NotAVersion" CACHE STRING "last SDL version used")
  mark_as_advanced (FORCE SDL_VERSION_LAST)
 endif (NOT DEFINED SDL_VERSION_LAST)
 if (NOT (${SDL_VERSION} MATCHES ${SDL_VERSION_LAST}))
  unset (SDL_USE CACHE)
  unset (SDL2_USE CACHE)
  unset (SDL3_USE CACHE)
  set (SDL_VERSION_LAST ${SDL_VERSION} CACHE STRING "Updating SDL Version Option" FORCE)
 endif (NOT (${SDL_VERSION} MATCHES ${SDL_VERSION_LAST}))

 if (${SDL_VERSION} STREQUAL "SDL")
  message (STATUS "using SDL")
  set (SDL_USE ON CACHE STRING "use SDL")
  mark_as_advanced (FORCE SDL_USE)
  add_definitions (-DSDL_USE)
  set (SDL_INCLUDE_DIRS ${SDL1_INCLUDE_DIRS})
  set (SDL_LIBRARIES ${SDL1_LIBRARIES})
  set (SDL_LIB_DIR ${SDL1_LIB_DIR})
 elseif (${SDL_VERSION} STREQUAL "SDL2")
  message (STATUS "using SDL2")
  set (SDL2_USE ON CACHE STRING "use SDL2")
  mark_as_advanced (FORCE SDL2_USE)
  add_definitions (-DSDL2_USE)
  set (SDL_INCLUDE_DIRS ${SDL2_INCLUDE_DIRS})
  set (SDL_LIBRARIES ${SDL2_LIBRARIES})
  set (SDL_LIB_DIR ${SDL2_LIB_DIR})
 elseif (${SDL_VERSION} STREQUAL "SDL3")
  message (STATUS "using SDL3")
  set (SDL3_USE ON CACHE STRING "use SDL3")
  mark_as_advanced (FORCE SDL3_USE)
  add_definitions (-DSDL3_USE)
  set (SDL_INCLUDE_DIRS ${SDL3_INCLUDE_DIRS})
  set (SDL_LIBRARIES ${SDL3_LIBRARIES})
  set (SDL_LIB_DIR ${SDL3_LIB_DIR})
 else ()
  message (FATAL_ERROR "invalid/unknown SDL version, aborting")
 endif ()

 if (SDL_USE)
  set (SDL_INCLUDE_DIRS "${SDL1_INCLUDE_DIRS};${SDL1_TTF_INCLUDE_DIRS};${SDL1_MIXER_INCLUDE_DIRS}")
  set (SDL_LIBRARIES "${SDL1_LIBRARIES};${SDL1_TTF_LIBRARIES};${SDL1_MIXER_LIBRARIES}")
  set (SDL_LIB_DIR "${SDL1_LIB_DIR};${SDL1_TTF_LIB_DIR};${SDL1_MIXER_LIB_DIR}")
 endif (SDL_USE)

 if (SDL2_USE)
  set (SDL_INCLUDE_DIRS "${SDL2_INCLUDE_DIRS};${SDL2_TTF_INCLUDE_DIRS};${SDL2_MIXER_INCLUDE_DIRS}")
  set (SDL_LIBRARIES "${SDL2_LIBRARIES};${SDL2_TTF_LIBRARIES};${SDL2_MIXER_LIBRARIES}")
  set (SDL_LIB_DIR "${SDL2_LIB_DIR};${SDL2_TTF_LIB_DIR};${SDL2_MIXER_LIB_DIR}")
 endif (SDL2_USE)

 if (SDL3_USE)
  set (SDL_INCLUDE_DIRS "/usr/local/include/SDL3;/usr/local/include/SDL3_ttf;/usr/local/include/SDL3_mixer")
#  set (SDL_INCLUDE_DIRS "${SDL3_INCLUDE_DIRS};${SDL3_TTF_INCLUDE_DIRS};${SDL3_MIXER_INCLUDE_DIRS}")
  set (SDL_LIBRARIES "${SDL3_LIBRARIES};${SDL3_TTF_LIBRARIES};${SDL3_MIXER_LIBRARIES}")
  set (SDL_LIB_DIR "${SDL3_LIB_DIR};${SDL3_TTF_LIB_DIR};${SDL3_MIXER_LIB_DIR}")
 endif (SDL3_USE)
endif (SDL_SUPPORT)

# *NOTE*: unify include dirs and libraries; includers use SDL_INCLUDE_DIRS and SDL_LIBRARIES
# *NOTE*: if no version has been selected, but SDL is supported, prefer SDL3 over SDL2 over SDL
if (SDL_SUPPORT AND NOT SDL_USE AND NOT SDL2_USE AND NOT SDL3_USE)
 message (WARNING "SDL version not selected, preferring SDL3 over SDL2 over SDL")
 if (SDL3_FOUND)
  set (SDL_INCLUDE_DIRS "${SDL3_INCLUDE_DIRS};${SDL3_TTF_INCLUDE_DIRS};${SDL3_MIXER_INCLUDE_DIRS}")
  set (SDL_LIBRARIES "${SDL3_LIBRARIES};${SDL3_TTF_LIBRARIES};${SDL3_MIXER_LIBRARIES}")
  set (SDL_LIB_DIR "${SDL3_LIB_DIR};${SDL3_TTF_LIB_DIR};${SDL3_MIXER_LIB_DIR}")
 elseif (SDL2_FOUND)
  set (SDL_INCLUDE_DIRS "${SDL2_INCLUDE_DIRS};${SDL2_TTF_INCLUDE_DIRS};${SDL2_MIXER_INCLUDE_DIRS}")
  set (SDL_LIBRARIES "${SDL2_LIBRARIES};${SDL2_TTF_LIBRARIES};${SDL2_MIXER_LIBRARIES}")
  set (SDL_LIB_DIR "${SDL2_LIB_DIR};${SDL2_TTF_LIB_DIR};${SDL2_MIXER_LIB_DIR}")
 elseif (SDL1_FOUND)
  set (SDL_INCLUDE_DIRS "${SDL1_INCLUDE_DIRS};${SDL1_TTF_INCLUDE_DIRS};${SDL1_MIXER_INCLUDE_DIRS}")
  set (SDL_LIBRARIES "${SDL1_LIBRARIES};${SDL1_TTF_LIBRARIES};${SDL1_MIXER_LIBRARIES}")
  set (SDL_LIB_DIR "${SDL1_LIB_DIR};${SDL1_TTF_LIB_DIR};${SDL1_MIXER_LIB_DIR}")
 endif ()
endif (SDL_SUPPORT AND NOT SDL_USE AND NOT SDL2_USE AND NOT SDL3_USE)
