 endif (VCPKG_SUPPORT)
 if (NOT SDL_FOUND)
  find_path (SDL1_INCLUDE_DIRS NAMES SDL.h
             PATHS "$ENV{LIB_ROOT}/SDL"
             PATH_SUFFIXES include)
