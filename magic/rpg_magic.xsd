    <xsd:sequence>
      <xsd:element name="rounds" type="xsd:unsignedInt" minOccurs="0" maxOccurs="1"/>
    </xsd:sequence>
    <xsd:attribute name="action" type="RPG_Common_ActionType_Type" use="optional" default="ACTION_STANDARD"/>
